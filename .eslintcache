Selector } from \"react-redux\"\nimport { bookRoom } from \"../store/actions/actionHotel\"\nimport {Button} from 'react-bootstrap'\nimport Icons from '../components/icons'\n\nexport default function RoomDetail() {\n  const dispatch = useDispatch()\n  const { roomType } = useParams()\n  const [userEmail, setEmail] = useState(\"\")\n  const [error, setError] = useState(\"\")\n  const hotel = useSelector(state=>state.hotel)\n\n  function bookHandler() {\n    if (!userEmail) {\n      setError(\"Email cannot be empty\")\n    }else{\n      dispatch(bookRoom(userEmail))\n    }\n  }\n  \n  useEffect(()=>{\n    console.log(hotel,'dari bookrom');\n  },[])\n\n  function getRoomPrice(){\n    return hotel.room_type.filter(item=> item.type=== roomType)[0].price\n  }\n\n  return (\n    <div  style={{\n      backgroundImage:\n        `linear-gradient(to left bottom, rgba(245, 246, 252, 0.52), rgba(245, 246, 252, 0.52), rgba(245, 246, 252, 0.52),rgba(35, 8, 53, 1), rgba(35, 8, 53, 1)),\n        url('https://www.jmberlin.de/sites/default/files/glashof-front_keyvisual.jpg')`,\n      height: '100vh',\n      backgroundPosition: \"center\",\n      backgroundRepeat: \"no-repeat\",\n      backgroundSize: \"100% 100%\",\n      backgroundAttachment: \"fixed\",\n      width: \"100%\"}}\n    >\n      <div className='full-height row'>\n        <div className=\"col-9\">\n          {error &&\n                <div className=\"alert alert-danger\" role=\"alert\">\n                  {error}\n                </div>\n              }\n          <iframe src={`http://hotelimage.s3-ap-southeast-1.amazonaws.com/${roomType}/room.html`} width='100%' height='100%' frameborder=\"0\"></iframe>\n        </div>\n        <div className=\"col-3 justify-content-center\">\n          <h4 className='mt-5 text-center'>Room Informations</h4>\n          <p style={{textTransform:'capitalize'}} className=\"text-center\">Type : {roomType} Room</p>\n          <p style={{textTransform:'capitalize'}} className=\"text-center\">Price : Rp. {getRoomPrice()}</p>\n          <p className=\"mt-3 ml-5\" style={{fontSize: 12}}>Enter your email to book this room</p>\n          <input \n            type='email' \n            className='mb-2 ml-4 form-control' \n            name='emailAddress' \n            placeholder='Enter your email address...' \n            onChange={(e) => setEmail(e.target.value)} \n            style={{width: \"80%\", height: 35, borderColor: '#231535'}}\n          />\n          <Button onClick={bookHandler} className=\"ml-4\" style={{width: \"80%\"}}>Book This Room</Button>\n          <p className=\"mt-3 text-center\" style={{fontWeight: 'bold'}}>Facilities Included</p>\n          <Icons/>\n        </div>\n      </div>\n    </div>\n  )\n}","/home/amy/Documents/Phase 3/Final Project/VRHotelTour-WebClient/src/pages/Dashboard.jsx",["57","58","59","60","61","62","63"],"/home/amy/Documents/Phase 3/Final Project/VRHotelTour-WebClient/src/store/index.js",[],"/home/amy/Documents/Phase 3/Final Project/VRHotelTour-WebClient/src/store/actions/actionHotel.js",[],"/home/amy/Documents/Phase 3/Final Project/VRHotelTour-WebClient/src/pages/HotelDetail.jsx",["64","65","66"],"import React, { useEffect } from 'react'\nimport { useSelector, useDispatch } from 'react-redux'\nimport { fetchHotelById } from '../store/actions/actionHotel'\nimport { useHistory } from 'react-router-dom'\nimport { Button, Container } from 'react-bootstrap'\n\nimport FacilitiesIcon from '../components/FacilitiesIcon'\n\nexport default function HotelDetail() {\n  const dispatch = useDispatch()\n  const history = useHistory()\n  const hotel = useSelector((state) => state.hotel)\n\n  useEffect(() => {\n    dispatch(fetchHotelById('5fda55be9acbac7ff019610d'))\n  }, [])\n\n  function facilitiesClick(idx) {\n    history.push(`/facility/${idx}`)\n  }\n\n  if (!hotel.facilities) {\n    return (\n      <div>\n        <h1>LOADING.....</h1>\n      </div>\n    )\n  }\n  return (\n    <div className=\"ml-5 mr-5 mt-3\" style={{justif