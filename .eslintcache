t RoomDetail from './pages/RoomDetail'\nimport Dashboard from './pages/Dashboard'\n\nfunction App() {\n  return (\n    <Provider store={store}>\n      <BrowserRouter>\n        <Nav className=\"justify-content-start fixed-top\" style={{backgroundImage: 'linear-gradient(to left, rgba(245, 246, 252, 0.52), rgba(82, 58, 115, 1), rgba(35, 8, 53, 1), rgba(35, 8, 53, 1))'}}>\n          <Nav.Item as={Link} to='/'>\n            <div className=\"d-flex flex-row mt-3 ml-5\">\n              <h2 style={{fontStyle: 'italic', color: 'white'}}>Inep</h2><h2 style={{fontWeight: 'bold', color: 'white'}}>Inn</h2>\n            </div>\n          </Nav.Item>\n        </Nav>\n        <Switch>\n          <Route path='/' exact>\n            <Dashboard/>\n          </Route>\n          <Route path='/facility/:idx'>\n            <FacilityDetail />\n          </Route>\n          <Route path='/room/:roomType'>\n            <RoomDetail />\n          </Route>\n        </Switch>\n      </BrowserRouter>\n    </Provider>\n  );\n}\n\nexport default App;\n","/home/amy/Documents/Phase 3/Final Project/VRHotelTour-WebClient/src/pages/FacilityDetail.jsx",[],"/home/amy/Documents/Phase 3/Final Project/VRHotelTour-WebClient/src/pages/RoomDetail.jsx",["54","55","56","57"],"import React, { useEffect, useState } from 'react'\nimport { Link, useParams } from 'react-router-dom'\nimport { Container } from 'react-bootstrap'\nimport { useDispatch, useSelector } from \"react-redux\"\nimport { bookRoom } from \"../store/actions/actionHotel\"\nimport {Button} from 'react-bootstrap'\nimport Icons from '../components/icons'\n\nexport default function RoomDetail() {\n  const dispatch = useDispatch()\n  const { roomType } = useParams()\n  const [userEmail, setEmail] = useState(\"\")\n  const [error, setError] = useState(\"\")\n  const hotel = useSelector(state=>state.hotel)\n\n  function bookHandler() {\n    if (!userEmail) {\n      setError(\"Email cannot be empty\")\n    }else{\n      dispatch(bookRoom(userEmail))\n    }\n  }\n  \n  useEffect(()=>{\n    console.log(hotel,'dari bookrom');\n  },[])\n\n  function getRoomPrice(){\n    return hotel.room_type.filter(item=> item.type=== roomType)[0].price\n  }\n\n  return (\n    <div  style={{\n      backgroundImage:\n        `linear-gradient(to left bottom, rgba(245, 246, 252, 0.52), rgba(245, 246, 252, 0.52), rgba(245, 246, 252, 0.52),rgba(35, 8, 53, 1), rgba(35, 8, 53, 1)),\n        url('https://www.jmberlin.de/sites/default/files/glashof-front_keyvisual.jpg')`,\n      height: '100vh',\n      backgroundPosition: \"center\",\n      backgroundRepeat: \"no-repeat\",\n      backgroundSize: \"100% 100%\",\n      backgroundAttachment: \"fixed\",\n      width: \"100%\"}}\n    >\n      <div className='full-height row'>\n        <div className=\"col-9\">\n          {error &&\n                <div className=\"alert alert-danger\" role=\"alert\">\n                  {error}\n                </div>\n              }\n          <iframe src={`http://hotelimage.s3-ap-southeast-1.amazonaws.com/${roomType}/room.html`} width='100%' height='100%' frameborder=\"0\"></iframe>\n        </div>\n        <div className=\"col-3 justify-content-center\">\n          <h4 className='mt-5 text-center'>Room Informations</h4>\n          <p style={{textTransform:'capitalize'}} className=\"text-center\">Type : {roomType} Room</p>\n          <p style={{textTransform:'capitalize'}} className=\"text-center\">Price : Rp. {getRoomPrice()}</p>\n          <p className=\"mt-3 ml-5\" style={{fontSize: 12}}>Enter your email to book this room</p>\n          <input \n            type='email' \n            className='mb-2 ml-4 form-control' \n            name='emailAddress' \n            placeholder='Enter your email address...' \n            onChange={(e) => setEmail(e.target.value)} \n            style={{width: \"80%\", height: 35, borderColor: '#231535'}}\n          />\n          <Button onClick={bookHandler} className=\"ml-4\" style={{width: \"80%\"}}>Book This Room</Button>\n      